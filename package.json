{
  "name": "@khanonjs/engine",
  "version": "0.0.25",
  "description": "A typescript game engine extending Babylon.js",
  "private": false,
  "author": {
    "name": "Lorenzo Portillo"
  },
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Khanon-js/khanon.js.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && tsc && cpy package.json ./dist",
    "build:docs": "typedoc --options typedoc.json",
    "publish:docs": "node ./publish/publish-docs.mjs",
    "publish:validate-version": "node ./publish/validate-version.js",
    "test": "cross-env TS_NODE_PROJECT='./tsconfig.mocha.json' mocha -r ./mocha-setup.js -r ts-node/register 'src/**/*.spec.ts'",
    "link": "cpy package.json ./src && cd ./src && npm link",
    "unlink": "cd ./src && rimraf node_modules && rimraf package.json && rimraf package-lock.json && npm unlink",
    "clean": "rimraf dist",
    "webpack-bundle-analyze": "webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json",
    "check-circular-refs": "dpdm ./src/index.ts",
    "lint": "eslint ./src/**/*.*",
    "lint:fix": "eslint ./src/**/*.* --fix"
  },
  "devDependencies": {
    "@babylonjs/core": "^6.27.1",
    "@babylonjs/gui": "^6.27.1",
    "@babylonjs/gui-editor": "^6.27.1",
    "@babylonjs/inspector": "^6.27.1",
    "@babylonjs/loaders": "^6.27.1",
    "@babylonjs/materials": "^6.27.1",
    "@babylonjs/serializers": "^6.27.1",
    "@mxssfd/typedoc-theme": "file:mxssfd-typedoc-theme",
    "@types/chai": "^4.3.9",
    "@types/mocha": "^10.0.3",
    "@types/node": "^17.0.45",
    "@types/sinon": "^10.0.20",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "chai": "^4.3.10",
    "clean-webpack-plugin": "^4.0.0",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "html-webpack-plugin": "^5.5.3",
    "mocha": "^10.2.0",
    "recursive-copy": "^2.0.14",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "semver": "^7.5.4",
    "sinon": "^15.2.0",
    "string-replace-loader": "^3.1.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^3.11.3"
  },
  "peerDependencies": {
    "@babylonjs/core": "^6.2.0",
    "@babylonjs/gui": "^6.2.0",
    "@babylonjs/inspector": "^6.2.0",
    "rxjs": "^7.1.0"
  },
  "dependencies": {
    "form-data": "^4.0.0"
  }
}