{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "strict": true,
    "strictPropertyInitialization": false,
    "strictNullChecks": false,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": false
  },
  "exclude": [
    "src/**/*.spec.ts"
  ]
}
